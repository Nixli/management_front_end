{"ast":null,"code":"import axios from 'axios';\nimport dayjs from 'dayjs';\nexport default {\n  data() {\n    return {\n      statue0: false,\n      statue1: false,\n      statue2: false,\n      totalshouAmountSum: 0,\n      totalfuAmountSum: 0,\n      VAT: []\n    };\n  },\n  methods: {\n    async getxxList() {\n      const res = await axios({\n        method: \"get\",\n        url: \"/depreciationDetail\",\n        params: {\n          pageno: this.pageno,\n          pagesize: this.pagesize,\n          bookID: this.bookID,\n          employeeID: this.employeeID,\n          ...this.params\n        }\n      });\n\n      // 修改日期格式\n      this.list = res.data.data.list.map(item => {\n        return {\n          ...item,\n          ctime: dayjs(item.ctime).format('YYYY-MM-DD'),\n          utime: dayjs(item.utime).format('YYYY-MM-DD')\n        };\n      });\n      this.total = res.data.data.total;\n    },\n    isDayu0(totalshouAmountSum, totalfuAmountSum) {\n      if (totalshouAmountSum - totalfuAmountSum < 0) {\n        return 0;\n      } else {\n        return (totalshouAmountSum - totalfuAmountSum) * 0.13;\n      }\n    },\n    update0() {\n      this.statue0 = true;\n    },\n    update1() {\n      this.statue1 = true;\n    },\n    update2() {\n      this.statue2 = true;\n    },\n    async getShouVoucherList() {\n      const currentYearMonth = dayjs().format('YYYY-MM');\n      const res = await axios({\n        url: \"/voucher\",\n        method: \"get\",\n        params: {\n          bookID: localStorage.getItem('bookID'),\n          voucherWord: '收'\n        }\n      });\n      this.VAT = res.data.data.list.map(item => ({\n        ...item,\n        cime: dayjs(item.cime).format('YYYY-MM')\n      }));\n      this.VAT = this.VAT.filter(item => {\n        // 比较年份月份是否与当前年份月份一致\n        return item.cime === currentYearMonth;\n      });\n      console.log(this.VAT);\n      this.totalshouAmountSum = this.VAT.reduce((accumulator, currentValue) => {\n        // 将字符串形式的金额转换为浮点数并累计\n        const amount = parseFloat(currentValue.totalAmount.replace(',', '')) || 0;\n        return accumulator + amount;\n      }, 0);\n    },\n    async getFuVoucherList() {\n      const currentYearMonth = dayjs().format('YYYY-MM');\n      const res = await axios({\n        url: \"/voucher\",\n        method: \"get\",\n        params: {\n          bookID: localStorage.getItem('bookID'),\n          voucherWord: '付'\n        }\n      });\n      this.VAT = res.data.data.list.map(item => ({\n        ...item,\n        cime: dayjs(item.cime).format('YYYY-MM')\n      }));\n      this.VAT = this.VAT.filter(item => {\n        // 比较年份月份是否与当前年份月份一致\n        return item.cime === currentYearMonth;\n      });\n      this.totalfuAmountSum = this.VAT.reduce((accumulator, currentValue) => {\n        // 将字符串形式的金额转换为浮点数并累计\n        const amount = parseFloat(currentValue.totalAmount.replace(',', '')) || 0;\n        return accumulator + amount;\n      }, 0);\n    }\n  },\n  created() {\n    this.getShouVoucherList();\n    this.getFuVoucherList();\n    this.getxxList();\n  }\n};","map":{"version":3,"names":["axios","dayjs","data","statue0","statue1","statue2","totalshouAmountSum","totalfuAmountSum","VAT","methods","getxxList","res","method","url","params","pageno","pagesize","bookID","employeeID","list","map","item","ctime","format","utime","total","isDayu0","update0","update1","update2","getShouVoucherList","currentYearMonth","localStorage","getItem","voucherWord","cime","filter","console","log","reduce","accumulator","currentValue","amount","parseFloat","totalAmount","replace","getFuVoucherList","created"],"sources":["src/views/carryover/CarryOver.vue"],"sourcesContent":["<template>\r\n    <el-card class=\"test\">\r\n        <div slot=\"header\"><span>期末结转</span></div>\r\n        <el-row :gutter=\"20\">\r\n            <el-col :span=\"8\">\r\n                <el-card class=\"box-card\">\r\n                    <div slot=\"header\" class=\"clearfix\">\r\n                        <span>计提折旧</span>\r\n                        <el-button style=\"float: right; padding: 3px 0\" type=\"text\" @click=\"update0()\">测算金额</el-button>\r\n                    </div>\r\n                    <div class=\"text item\" v-if=\"statue0 === true\">\r\n                        {{ 5000 }}\r\n                    </div>\r\n                </el-card>\r\n            </el-col>\r\n            <el-col :span=\"8\">\r\n                <el-card class=\"box-card\">\r\n                    <div slot=\"header\" class=\"clearfix\">\r\n                        <span>计提增值税</span>\r\n                        <el-button style=\"float: right; padding: 3px 0\" type=\"text\" @click=\"update1()\">测算金额</el-button>\r\n                    </div>\r\n                    <div class=\"text item\" v-if=\"statue1 === true\">\r\n                        {{ parseFloat((totalshouAmountSum / 1.13 * 0.13).toFixed(2)) }}\r\n                    </div>\r\n                </el-card>\r\n            </el-col>\r\n            <el-col :span=\"8\">\r\n                <el-card class=\"box-card\">\r\n                    <div slot=\"header\" class=\"clearfix\">\r\n                        <span>计提所得税</span>\r\n                        <el-button style=\"float: right; padding: 3px 0\" type=\"text\" @click=\"update2()\">测算金额</el-button>\r\n                    </div>\r\n                    <div class=\"text item\" v-if=\"statue2 === true\">\r\n                        {{ isDayu0(this.totalshouAmountSum, this.totalfuAmountSum) }}\r\n                    </div>\r\n                </el-card>\r\n            </el-col>\r\n        </el-row>\r\n    </el-card>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport dayjs from 'dayjs';\r\n\r\nexport default {\r\n\r\n    data() {\r\n        return {\r\n            statue0: false,\r\n            statue1: false,\r\n            statue2: false,\r\n            totalshouAmountSum: 0,\r\n            totalfuAmountSum: 0,\r\n            VAT: [],\r\n        }\r\n    },\r\n    methods: {\r\n\r\n        async getxxList() {\r\n            const res = await axios({\r\n                method: \"get\",\r\n                url: \"/depreciationDetail\",\r\n                params: {\r\n                    pageno: this.pageno,\r\n                    pagesize: this.pagesize,\r\n                    bookID: this.bookID,\r\n                    employeeID: this.employeeID,\r\n                    ...this.params,\r\n                },\r\n            });\r\n\r\n            // 修改日期格式\r\n            this.list = res.data.data.list.map(item => {\r\n                return {\r\n                    ...item,\r\n                    ctime: dayjs(item.ctime).format('YYYY-MM-DD'),\r\n                    utime: dayjs(item.utime).format('YYYY-MM-DD'),\r\n                };\r\n            });\r\n            this.total = res.data.data.total;\r\n        },\r\n\r\n        isDayu0(totalshouAmountSum, totalfuAmountSum) {\r\n            if (totalshouAmountSum - totalfuAmountSum < 0) {\r\n                return 0;\r\n            } else {\r\n                return (totalshouAmountSum - totalfuAmountSum) * 0.13;\r\n            }\r\n        },\r\n        update0() {\r\n            this.statue0 = true;\r\n        },\r\n        update1() {\r\n            this.statue1 = true;\r\n        },\r\n        update2() {\r\n            this.statue2 = true;\r\n        },\r\n        async getShouVoucherList() {\r\n            const currentYearMonth = dayjs().format('YYYY-MM');\r\n            const res = await axios({\r\n                url: \"/voucher\",\r\n                method: \"get\",\r\n                params: {\r\n                    bookID: localStorage.getItem('bookID'),\r\n                    voucherWord: '收'\r\n                }\r\n            })\r\n            this.VAT = res.data.data.list.map(item => ({\r\n                ...item,\r\n                cime: dayjs(item.cime).format('YYYY-MM')\r\n            }))\r\n            this.VAT = this.VAT.filter(item => {\r\n                // 比较年份月份是否与当前年份月份一致\r\n                return item.cime === currentYearMonth;\r\n            });\r\n            console.log(this.VAT)\r\n\r\n            this.totalshouAmountSum = this.VAT.reduce((accumulator, currentValue) => {\r\n                // 将字符串形式的金额转换为浮点数并累计\r\n                const amount = parseFloat(currentValue.totalAmount.replace(',', '')) || 0;\r\n                return accumulator + amount;\r\n            }, 0);\r\n        },\r\n\r\n        async getFuVoucherList() {\r\n            const currentYearMonth = dayjs().format('YYYY-MM');\r\n            const res = await axios({\r\n                url: \"/voucher\",\r\n                method: \"get\",\r\n                params: {\r\n                    bookID: localStorage.getItem('bookID'),\r\n                    voucherWord: '付'\r\n                }\r\n            })\r\n            this.VAT = res.data.data.list.map(item => ({\r\n                ...item,\r\n                cime: dayjs(item.cime).format('YYYY-MM')\r\n            }))\r\n            this.VAT = this.VAT.filter(item => {\r\n                // 比较年份月份是否与当前年份月份一致\r\n                return item.cime === currentYearMonth;\r\n            });\r\n\r\n            this.totalfuAmountSum = this.VAT.reduce((accumulator, currentValue) => {\r\n                // 将字符串形式的金额转换为浮点数并累计\r\n                const amount = parseFloat(currentValue.totalAmount.replace(',', '')) || 0;\r\n                return accumulator + amount;\r\n            }, 0);\r\n        },\r\n\r\n    },\r\n    created() {\r\n        this.getShouVoucherList();\r\n        this.getFuVoucherList();\r\n        this.getxxList();\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n::v-deep .el-card__header {\r\n    background-color: rgb(249, 251, 252);\r\n}\r\n\r\n.text {\r\n    font-size: 24px;\r\n    text-align: center;\r\n    justify-content: center;\r\n    margin-top: 55px;\r\n\r\n}\r\n\r\n.item {\r\n    margin-bottom: 18px;\r\n}\r\n\r\n.clearfix:before,\r\n.clearfix:after {\r\n    display: table;\r\n    content: \"\";\r\n}\r\n\r\n.clearfix:after {\r\n    clear: both\r\n}\r\n\r\n.box-card {\r\n    width: 350px;\r\n    height: 270px;\r\n}\r\n\r\n.el-row {\r\n    margin-bottom: 20px;\r\n\r\n    &:last-child {\r\n        margin-bottom: 0;\r\n    }\r\n}\r\n\r\n.el-col {\r\n    border-radius: 4px;\r\n}\r\n\r\n.bg-purple-dark {\r\n    background: #99a9bf;\r\n}\r\n\r\n.bg-purple {\r\n    background: #d3dce6;\r\n}\r\n\r\n.bg-purple-light {\r\n    background: #e5e9f2;\r\n}\r\n\r\n.grid-content {\r\n    border-radius: 4px;\r\n    min-height: 36px;\r\n}\r\n\r\n.row-bg {\r\n    padding: 10px 0;\r\n    background-color: #f9fafc;\r\n}\r\n</style>"],"mappings":"AA0CA,OAAAA,KAAA;AACA,OAAAC,KAAA;AAEA;EAEAC,KAAA;IACA;MACAC,OAAA;MACAC,OAAA;MACAC,OAAA;MACAC,kBAAA;MACAC,gBAAA;MACAC,GAAA;IACA;EACA;EACAC,OAAA;IAEA,MAAAC,UAAA;MACA,MAAAC,GAAA,SAAAX,KAAA;QACAY,MAAA;QACAC,GAAA;QACAC,MAAA;UACAC,MAAA,OAAAA,MAAA;UACAC,QAAA,OAAAA,QAAA;UACAC,MAAA,OAAAA,MAAA;UACAC,UAAA,OAAAA,UAAA;UACA,QAAAJ;QACA;MACA;;MAEA;MACA,KAAAK,IAAA,GAAAR,GAAA,CAAAT,IAAA,CAAAA,IAAA,CAAAiB,IAAA,CAAAC,GAAA,CAAAC,IAAA;QACA;UACA,GAAAA,IAAA;UACAC,KAAA,EAAArB,KAAA,CAAAoB,IAAA,CAAAC,KAAA,EAAAC,MAAA;UACAC,KAAA,EAAAvB,KAAA,CAAAoB,IAAA,CAAAG,KAAA,EAAAD,MAAA;QACA;MACA;MACA,KAAAE,KAAA,GAAAd,GAAA,CAAAT,IAAA,CAAAA,IAAA,CAAAuB,KAAA;IACA;IAEAC,QAAApB,kBAAA,EAAAC,gBAAA;MACA,IAAAD,kBAAA,GAAAC,gBAAA;QACA;MACA;QACA,QAAAD,kBAAA,GAAAC,gBAAA;MACA;IACA;IACAoB,QAAA;MACA,KAAAxB,OAAA;IACA;IACAyB,QAAA;MACA,KAAAxB,OAAA;IACA;IACAyB,QAAA;MACA,KAAAxB,OAAA;IACA;IACA,MAAAyB,mBAAA;MACA,MAAAC,gBAAA,GAAA9B,KAAA,GAAAsB,MAAA;MACA,MAAAZ,GAAA,SAAAX,KAAA;QACAa,GAAA;QACAD,MAAA;QACAE,MAAA;UACAG,MAAA,EAAAe,YAAA,CAAAC,OAAA;UACAC,WAAA;QACA;MACA;MACA,KAAA1B,GAAA,GAAAG,GAAA,CAAAT,IAAA,CAAAA,IAAA,CAAAiB,IAAA,CAAAC,GAAA,CAAAC,IAAA;QACA,GAAAA,IAAA;QACAc,IAAA,EAAAlC,KAAA,CAAAoB,IAAA,CAAAc,IAAA,EAAAZ,MAAA;MACA;MACA,KAAAf,GAAA,QAAAA,GAAA,CAAA4B,MAAA,CAAAf,IAAA;QACA;QACA,OAAAA,IAAA,CAAAc,IAAA,KAAAJ,gBAAA;MACA;MACAM,OAAA,CAAAC,GAAA,MAAA9B,GAAA;MAEA,KAAAF,kBAAA,QAA