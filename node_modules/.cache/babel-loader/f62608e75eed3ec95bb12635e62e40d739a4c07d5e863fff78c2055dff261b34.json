.把得到的数据放在data下\r\n            this.list = res.data.data.list;\r\n            this.total = res.data.data.total;\r\n        },\r\n        // 点击搜索\r\n        search() {\r\n            this.getList()\r\n        },\r\n        // 页码改变\r\n        pagechange(pageno) {\r\n            // 条件改变\r\n            this.pageno = pageno\r\n            // 重新请求数据\r\n            this.getList()\r\n        },\r\n    },\r\n\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.row-box {\r\n    width: 100%;\r\n    display: flex;\r\n    margin-top: 20px;\r\n    margin-bottom: 8px;\r\n    justify-content: center;\r\n}\r\n</style>"],"mappings":"AA2CA,OAAAA,KAAA;AAEA;EAEAC,KAAA;IACA;MACAC,IAAA;MACA;MACAC,MAAA;MACAC,QAAA;MACAC,KAAA;MACA;MACAC,MAAA;QACAC,SAAA;MACA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;EACA;EAEAC,OAAA;IACA,MAAAD,QAAA;MACA,MAAAE,GAAA,SAAAX,KAAA;QACAY,MAAA;QACAC,GAAA;QACAP,MAAA;UACAH,MAAA,OAAAA,MAAA;UACAC,QAAA,OAAAA,QAAA;UACA,QAAAE;QACA;MACA;MACA;MACA,KAAAJ,IAAA,GAAAS,GAAA,CAAAV,IAAA,CAAAA,IAAA,CAAAC,IAAA;MACA,KAAAG,KAAA,GAAAM,GAAA,CAAAV,IAAA,CAAAA,IAAA,CAAAI,KAAA;IACA;IACA;IACAS,OAAA;MACA,KAAAL,OAAA;IACA;IACA;IACAM,WAAAZ,MAAA;MACA;MACA,KAAAA,MAAA,GAAAA,MAAA;MACA;MACA,KAAAM,OAAA;IACA;EACA;AAEA"},"metadata":{},"sourceType":"module","externalDependencies":[]}