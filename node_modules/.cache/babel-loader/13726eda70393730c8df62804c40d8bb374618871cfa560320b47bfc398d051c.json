{"ast":null,"code":"import axios from \"axios\";\nimport dayjs from \"dayjs\";\nimport { isShallow } from \"vue\";\nexport default {\n  data() {\n    return {\n      DeclarationDate: '',\n      ReviewDate: '',\n      role: localStorage.getItem('role'),\n      list: [],\n      // 分页相关\n      pageno: 1,\n      pagesize: 5,\n      total: 0,\n      // 搜索条件\n      params: {\n        assetName: '',\n        brandModel: ''\n      },\n      // 控制弹框的显示与隐藏\n      dialogFormVisible: false,\n      // 新增表单的数据\n      userFormData: {\n        assetNumber: '',\n        assetName: '',\n        brandModel: '',\n        description: '',\n        originalValue: '',\n        netValue: '',\n        depreciationAmount: '',\n        accountBook: {\n          bookID: localStorage.getItem('bookID')\n        } // 使用关系属性名\n      },\n      // add： 新增   edit: '更新'\n      actionType: '',\n      id: '',\n      bookID: localStorage.getItem('bookID'),\n      isShow: false\n    };\n  },\n  created() {\n    this.getList();\n  },\n  methods: {\n    async getList() {\n      const res = await axios({\n        method: \"get\",\n        url: \"/depreciationDetail\",\n        params: {\n          pageno: this.pageno,\n          pagesize: this.pagesize,\n          bookID: this.bookID,\n          ...this.params\n        }\n      });\n\n      // 修改日期格式\n      this.list = res.data.data.list.map(item => {\n        return {\n          ...item,\n          ctime: dayjs(item.ctime).format('YYYY-MM-DD HH:mm:ss')\n        };\n      });\n      this.total = res.data.data.total;\n    },\n    // 点击搜索\n    search() {\n      this.getList();\n    },\n    // 页码改变\n    pagechange(pageno) {\n      // 条件改变\n      this.pageno = pageno;\n      // 重新请求数据\n      this.getList();\n    },\n    // 打开新增弹窗，\n    addOpen() {\n      // 打开弹窗\n      this.dialogFormVisible = true;\n      // 记录状态\n      this.actionType = 'add';\n      // 重置userFormData\n      this.userFormData = {\n        assetNumber: '',\n        assetName: '',\n        brandModel: '',\n        description: '',\n        originalValue: '',\n        netValue: '',\n        depreciationAmount: '',\n        accountBook: {\n          bookID: localStorage.getItem('bookID')\n        } // 使用关系属性名\n      };\n    },\n    //新增或修改固定资产\n    async submit() {\n      // 拷贝userFormData到data\n      const data = {\n        ...this.userFormData\n      };\n      // 修改的时候需要把id带过去\n      const res = await axios({\n        url: '/depreciationDetail',\n        method: 'post',\n        data: data\n      });\n      this.dialogFormVisible = false;\n      this.getList();\n    },\n    process(row) {\n      this.isShow = true;\n      this.DeclarationDate = row.ctime;\n      this.ReviewDate = row.utime;\n    }\n  }\n};","map":{"version":3,"names":["axios","dayjs","isShallow","data","DeclarationDate","ReviewDate","role","localStorage","getItem","list","pageno","pagesize","total","params","assetName","brandModel","dialogFormVisible","userFormData","assetNumber","description","originalValue","netValue","depreciationAmount","accountBook","bookID","actionType","id","isShow","created","getList","methods","res","method","url","map","item","ctime","format","search","pagechange","addOpen","submit","process","row","utime"],"sources":["src/views/fixedasset/PersonalDepreciation.vue"],"sourcesContent":["<template>\r\n    <div style=\"height: auto; width: auto;\">\r\n\r\n        <!-- 搜索新增区域 -->\r\n        <el-card>\r\n            <div style=\"display: flex; flex-direction: row;\">\r\n                <!-- 搜索区域 -->\r\n                <div style=\"flex: 8;\">\r\n                    <el-form :inline=\"true\">\r\n                        <el-form-item label=\"资产名称\">\r\n                            <el-input v-model=\"params.assetName\" placeholder=\"请输入资产名称\"></el-input>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"资产类型\">\r\n                            <el-input v-model=\"params.brandModel\" placeholder=\"请输入资产类型\"></el-input>\r\n                        </el-form-item>\r\n                        <el-form-item>\r\n                            <el-button @click=\"search\" type=\"primary\" icon=\"el-icon-search\">搜索</el-button>\r\n                        </el-form-item>\r\n                        <el-button type=\"primary\" @click=\"addOpen\">新增折旧申请</el-button>\r\n                    </el-form>\r\n                </div>\r\n\r\n            </div>\r\n        </el-card>\r\n\r\n        <!-- 折旧明细表 -->\r\n        <el-card style=\"margin-top: 20px;\">\r\n\r\n            <el-card>\r\n                <span style=\"font-size: 16px;font-weight: bold;font-family: 'Times New Roman', Times, serif;\">折旧明细表</span>\r\n            </el-card>\r\n\r\n            <el-table :data=\"list\" border style=\"width: 100%\">\r\n                <el-table-column prop=\"assetNumber\" label=\"资产编号\"> </el-table-column>\r\n                <el-table-column prop=\"assetName\" label=\"资产名称\"> </el-table-column>\r\n                <el-table-column prop=\"brandModel\" label=\"资产类型\"> </el-table-column>\r\n                <el-table-column prop=\"description\" label=\"品牌型号\"> </el-table-column>\r\n                <el-table-column prop=\"originalValue\" label=\"资产原值\"> </el-table-column>\r\n                <el-table-column prop=\"netValue\" label=\"资产净值\"> </el-table-column>\r\n                <el-table-column prop=\"depreciationAmount\" label=\"资产折旧金额\"> </el-table-column>\r\n                <el-table-column prop=\"ctime\" label=\"申请日期\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"isRetired\" label=\"状态\" align=\"center\">\r\n                    <template #default=\"{ row }\">\r\n                        {{ row.isRetired === 0 ? '待审核' : (row.isRetired === 1 ? '已通过' : '未通过') }}\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column fixed=\"right\" label=\"流程\">\r\n                    <template #default=\"{ row }\">\r\n                        <!-- 在这里使用 row 对象，表示整行的数据 -->\r\n                        <el-button type=\"text\" size=\"small\" @click=\"process(row)\">审批流程</el-button>\r\n                    </template>\r\n                </el-table-column>\r\n\r\n            </el-table>\r\n\r\n            <!-- 分页组件 -->\r\n            <el-pagination @current-change=\"pagechange\" background layout=\"prev, pager, next\" :total=\"total\"\r\n                :page-size=\"pagesize\" style=\"text-align: center; margin-top: 10px;\">\r\n            </el-pagination>\r\n        </el-card>\r\n\r\n        <!-- 弹窗区域 -->\r\n        <el-dialog :title=\"actionType == 'add' ? '新增折旧明细' : '编辑折旧明细'\" :visible.sync=\"dialogFormVisible\">\r\n            <el-form :model=\"userFormData\" class=\"demo-form-inline\" label-width=\"120px\">\r\n\r\n                <el-form-item label=\"资产编号\">\r\n                    <el-input v-model=\"userFormData.assetNumber\" placeholder=\"资产编号\"></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"资产名称\">\r\n                    <el-input v-model=\"userFormData.assetName\" placeholder=\"资产名称\"></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"资产类型\">\r\n                    <el-input v-model=\"userFormData.brandModel\" placeholder=\"资产类型\"></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"品牌型号\">\r\n                    <el-input v-model=\"userFormData.description\" placeholder=\"品牌型号\"></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"资产原值\">\r\n                    <el-input v-model=\"userFormData.originalValue\" placeholder=\"资产原值\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"资产净值\">\r\n                    <el-input v-model=\"userFormData.netValue\" placeholder=\"资产净值\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"资产折旧金额\">\r\n                    <el-input v-model=\"userFormData.depreciationAmount\" placeholder=\"资产折旧金额\"></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item>\r\n                    <el-button @click=\"submit\">提交</el-button>\r\n                </el-form-item>\r\n\r\n            </el-form>\r\n        </el-dialog>\r\n\r\n        <el-dialog :title=\"报销流程\" :visible.sync=\"isShow\">\r\n            <div class=\"block\">\r\n                <el-timeline>\r\n                    <el-timeline-item :timestamp=\"DeclarationDate\" placement=\"top\">\r\n                        <el-card>\r\n                            <h4>更新 Github 模板</h4>\r\n                            <p>王小虎 提交于 2018/4/12 20:46</p>\r\n                        </el-card>\r\n                    </el-timeline-item>\r\n                    <el-timeline-item :timestamp=\"ReviewDate\" placement=\"top\">\r\n                        <el-card>\r\n                            <h4>更新 Github 模板</h4>\r\n                            <p>王小虎 提交于 2018/4/3 20:46</p>\r\n                        </el-card>\r\n                    </el-timeline-item>\r\n                </el-timeline>\r\n            </div>\r\n        </el-dialog>\r\n\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\nimport dayjs from \"dayjs\";\r\nimport { isShallow } from \"vue\";\r\n\r\nexport default {\r\n\r\n    data() {\r\n        return {\r\n            DeclarationDate:'',\r\n            ReviewDate:'',\r\n            role: localStorage.getItem('role'),\r\n            list: [],\r\n            // 分页相关\r\n            pageno: 1,\r\n            pagesize: 5,\r\n            total: 0,\r\n            // 搜索条件\r\n            params: {\r\n                assetName: '',\r\n                brandModel: ''\r\n            },\r\n            // 控制弹框的显示与隐藏\r\n            dialogFormVisible: false,\r\n            // 新增表单的数据\r\n            userFormData: {\r\n                assetNumber: '',\r\n                assetName: '',\r\n                brandModel: '',\r\n                description: '',\r\n                originalValue: '',\r\n                netValue: '',\r\n                depreciationAmount: '',\r\n                accountBook: { bookID: localStorage.getItem('bookID') } // 使用关系属性名\r\n            },\r\n            // add： 新增   edit: '更新'\r\n            actionType: '',\r\n            id: '',\r\n            bookID: localStorage.getItem('bookID'),\r\n            isShow: false,\r\n        };\r\n    },\r\n    created() {\r\n        this.getList();\r\n        \r\n    },\r\n\r\n    methods: {\r\n        async getList() {\r\n            const res = await axios({\r\n                method: \"get\",\r\n                url: \"/depreciationDetail\",\r\n                params: {\r\n                    pageno: this.pageno,\r\n                    pagesize: this.pagesize,\r\n                    bookID: this.bookID,\r\n                    ...this.params,\r\n                },\r\n            });\r\n\r\n            // 修改日期格式\r\n            this.list = res.data.data.list.map(item => {\r\n                return {\r\n                    ...item,\r\n                    ctime: dayjs(item.ctime).format('YYYY-MM-DD HH:mm:ss'),\r\n                };\r\n            });\r\n            this.total = res.data.data.total;\r\n        },\r\n        // 点击搜索\r\n        search() {\r\n            this.getList()\r\n        },\r\n        // 页码改变\r\n        pagechange(pageno) {\r\n            // 条件改变\r\n            this.pageno = pageno\r\n            // 重新请求数据\r\n            this.getList()\r\n        },\r\n        // 打开新增弹窗，\r\n        addOpen() {\r\n            // 打开弹窗\r\n            this.dialogFormVisible = true\r\n            // 记录状态\r\n            this.actionType = 'add'\r\n            // 重置userFormData\r\n            this.userFormData = {\r\n                assetNumber: '',\r\n                assetName: '',\r\n                brandModel: '',\r\n                description: '',\r\n                originalValue: '',\r\n                netValue: '',\r\n                depreciationAmount: '',\r\n                accountBook: { bookID: localStorage.getItem('bookID') } // 使用关系属性名\r\n            }\r\n        },\r\n\r\n        //新增或修改固定资产\r\n        async submit() {\r\n            // 拷贝userFormData到data\r\n            const data = { ...this.userFormData }\r\n            // 修改的时候需要把id带过去\r\n            const res = await axios({\r\n                url: '/depreciationDetail',\r\n                method: 'post',\r\n                data: data\r\n            })\r\n            this.dialogFormVisible = false\r\n            this.getList()\r\n        },\r\n        process(row) {\r\n            this.isShow = true\r\n            this.DeclarationDate = row.ctime\r\n            this.ReviewDate = row.utime\r\n        }\r\n    },\r\n\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.row-box {\r\n    width: 100%;\r\n    display: flex;\r\n    margin-top: 20px;\r\n    margin-bottom: 8px;\r\n    justify-content: center;\r\n}\r\n</style>"],"mappings":"AA2HA,OAAAA,KAAA;AACA,OAAAC,KAAA;AACA,SAAAC,SAAA;AAEA;EAEAC,KAAA;IACA;MACAC,eAAA;MACAC,UAAA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA;MACAC,IAAA;MACA;MACAC,MAAA;MACAC,QAAA;MACAC,KAAA;MACA;MACAC,MAAA;QACAC,SAAA;QACAC,UAAA;MACA;MACA;MACAC,iBAAA;MACA;MACAC,YAAA;QACAC,WAAA;QACAJ,SAAA;QACAC,UAAA;QACAI,WAAA;QACAC,aAAA;QACAC,QAAA;QACAC,kBAAA;QACAC,WAAA;UAAAC,MAAA,EAAAjB,YAAA,CAAAC,OAAA;QAAA;MACA;MACA;MACAiB,UAAA;MACAC,EAAA;MACAF,MAAA,EAAAjB,YAAA,CAAAC,OAAA;MACAmB,MAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;EAEA;EAEAC,OAAA;IACA,MAAAD,QAAA;MACA,MAAAE,GAAA,SAAA/B,KAAA;QACAgC,MAAA;QACAC,GAAA;QACApB,MAAA;UACAH,MAAA,OAAAA,MAAA;UACAC,QAAA,OAAAA,QAAA;UACAa,MAAA,OAAAA,MAAA;UACA,QAAAX;QACA;MACA;;MAEA;MACA,KAAAJ,IAAA,GAAAsB,GAAA,CAAA5B,IAAA,CAAAA,IAAA,CAAAM,IAAA,CAAAyB,GAAA,CAAAC,IAAA;QACA;UACA,GAAAA,IAAA;UACAC,KAAA,EAAAnC,KAAA,CAAAkC,IAAA,CAAAC,KAAA,EAAAC,MAAA;QACA;MACA;MACA,KAAAzB,KAAA,GAAAmB,GAAA,CAAA5B,IAAA,CAAAA,IAAA,CAAAS,KAAA;IACA;IACA;IACA0B,OAAA;MACA,KAAAT,OAAA;IACA;IACA;IACAU,WAAA7B,MAAA;MACA;MACA,KAAAA,MAAA,GAAAA,MAAA;M