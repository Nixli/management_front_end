{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      accountBooksList: [],\n      dialogFormVisible: false,\n      userFormData: {\n        bookName: '',\n        companyName: '',\n        businessAddress: '',\n        industry: '',\n        contactPerson: '',\n        contactPhoneNumber: '',\n        indvatTypeustry: '',\n        notes: '',\n        ctime: ''\n      },\n      id: ''\n    };\n  },\n  created() {\n    this.getList();\n  },\n  methods: {\n    async getList() {\n      const res = await axios({\n        method: \"get\",\n        url: \"/accountBooks\"\n      });\n      this.accountBooksList = res.data.data;\n    },\n    viewAccountDetails(bookID, bookName) {\n      // 存储具体某个账套的bookID到localStorage\n      localStorage.setItem(\"bookID\", bookID);\n      localStorage.setItem(\"bookName\", bookName);\n      // 这里可以跳转到具体账套的详细信息页面或执行其他操作\n      // 示例：跳转到名为'/accountDetails'的路由\n      this.$router.push('/home');\n    }\n  }\n};","map":{"version":3,"names":["axios","data","accountBooksList","dialogFormVisible","userFormData","bookName","companyName","businessAddress","industry","contactPerson","contactPhoneNumber","indvatTypeustry","notes","ctime","id","created","getList","methods","res","method","url","viewAccountDetails","bookID","localStorage","setItem","$router","push"],"sources":["src/views/accountBooks/AccountBooks.vue"],"sourcesContent":["<template>\r\n    <el-card>\r\n\r\n        <el-card style=\"text-align:center\">\r\n            <span style=\"font-size: 24px;font-weight: bold;font-family: 'Times New Roman', Times, serif;\">账套管理</span>\r\n        </el-card>\r\n\r\n\r\n        <el-card style=\"margin-top:20px\">\r\n            <span style=\"font-size: 16px;font-weight: bold;font-family: 'Times New Roman', Times, serif;\">账套管理</span>\r\n            <el-button style=\"margin-left:20px\">新增账套</el-button>\r\n        </el-card>\r\n        <!-- 账套区域 -->\r\n        <el