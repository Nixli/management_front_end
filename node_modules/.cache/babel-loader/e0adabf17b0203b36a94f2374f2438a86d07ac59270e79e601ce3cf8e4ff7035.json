=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"allowance\" label=\"津贴\" width=\"120\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"bonus\" label=\"奖金\" width=\"120\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"subsidy\" label=\"补助\" width=\"120\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"deduction\" label=\"扣款\" width=\"120\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"personalIncomeTax\" label=\"个税\" width=\"120\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"actualSalary\" label=\"实发工资\" width=\"120\" align=\"center\">\r\n                </el-table-column>\r\n                <el-table-column prop=\"payoutDate\" label=\"工资发放日期\" align=\"center\"> </el-table-column>\r\n                <el-table-column fixed=\"right\" label=\"操作\" align=\"center\">\r\n                    <template #default=\"{ row }\">\r\n                        <el-button type=\"text\" size=\"small\" @click=\"edit(row)\">修改工资</el-button>\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column fixed=\"right\" label=\"生成凭证\" align=\"center\">\r\n                    <template #default=\"{ row }\">\r\n                        <el-button type=\"text\" size=\"small\">生成凭证</el-button>\r\n                    </template>\r\n                </el-table-column>\r\n            </el-table>\r\n\r\n            <!-- 分页组件 -->\r\n            <el-pagination @current-change=\"pagechange\" background layout=\"prev, pager, next\" :total=\"total\"\r\n                :page-size=\"pagesize\" style=\"text-align: center; margin-top: 10px;\">\r\n            </el-pagination>\r\n        </el-card>\r\n\r\n        <!-- 弹窗区域 -->\r\n        <el-dialog :title=\"actionType == 'add' ? '新增员工工资信息' : '编辑员工工资信息'\" :visible.sync=\"dialogFormVisible\">\r\n            <el-form :model=\"userFormData\" class=\"demo-form-inline\" label-width=\"120px\">\r\n\r\n                <el-form-item label=\"基本工资\">\r\n                    <el-input v-model=\"userFormData.baseSalary\" placeholder=\"基本工资\"></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"津贴\">\r\n                    <el-input v-model=\"userFormData.allowance\" placeholder=\"津贴\"></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"奖金\">\r\n                    <el-input v-model=\"userFormData.bonus\" placeholder=\"奖金\"></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"补助\">\r\n                    <el-input v-model=\"userFormData.subsidy\" placeholder=\"补助\"></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"扣款\">\r\n                    <el-input v-model=\"userFormData.deduction\" placeholder=\"扣款\"></el-input>\r\n\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"个税\">\r\n                    <el-input v-model=\"userFormData.personalIncomeTax\" placeholder=\"个税\" @change=\"calculatePersonalIncomeTax\"></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"实发工资\">\r\n                    <el-input v-model=\"userFormData.actualSalary\" placeholder=\"实发工资\"></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item>\r\n                    <el-button>提交</el-button>\r\n                </el-form-item>\r\n\r\n            </el-form>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\nimport dayjs from \"dayjs\"\r\nexport default {\r\n\r\n    data() {\r\n        return {\r\n            list: [],\r\n            // 分页相关\r\n            pageno: 1,\r\n            pagesize: 5,\r\n            total: 0,\r\n            // 搜索条件\r\n            params: {\r\n                assetName: dayjs().subtract(1, 'year').format('YYYY-MM-DD HH:mm:ss'),\r\n                brandModel: dayjs().format('YYYY-MM-DD HH:mm:ss')\r\n            },\r\n            userFormData: {\r\n                name: '',\r\n                baseSalary: 0.00,\r\n                allowance: 0.00,\r\n                bonus: 0.00,\r\n                subsidy: 0.00,\r\n                deduction: 0.00,\r\n                personalIncomeTax: 0.00,\r\n                actualSalary: 0.00\r\n            },\r\n            bookID: localStorage.getItem('bookID'),\r\n            employeeID: localStorage.getItem('employeeID'),\r\n            actionType: '',\r\n            id: '',\r\n            dialogFormVisible: false\r\n        };\r\n    },\r\n    created() {\r\n        this.getList()\r\n    },\r\n\r\n    methods: {\r\n        async getList() {\r\n\r\n            const formattedParams = {\r\n                ...this.params,\r\n                assetName: dayjs(this.params.assetName).format('YYYY-MM-DD HH:mm:ss'),\r\n                brandModel: dayjs(this.params.brandModel).format('YYYY-MM-DD HH:mm:ss'),\r\n            };\r\n            const res = await axios({\r\n                method: \"get\",\r\n                url: \"/managementSalary\",\r\n                params: {\r\n                    pageno: this.pageno,\r\n                    pagesize: this.pagesize,\r\n                    bookID: this.bookID,\r\n                    ...formattedParams\r\n                },\r\n            });\r\n            // 修改日期格式\r\n            this.list = res.data.data.list.map(item => {\r\n                return {\r\n                    ...item,\r\n                    payoutDate: dayjs(item.payoutDate).format('YYYY-MM-DD'),\r\n                };\r\n            });\r\n            console.log(this.list)\r\n            this.total = res.data.data.total;\r\n        },\r\n        // 点击搜索\r\n        search() {\r\n            this.getList()\r\n        },\r\n        // 页码改变\r\n        pagechange(pageno) {\r\n            // 条件改变\r\n            this.pageno = pageno\r\n            // 重新请求数据\r\n            this.getList()\r\n        },\r\n        // 打开新增弹窗，\r\n        addOpen() {\r\n            // 打开弹窗\r\n            this.dialogFormVisible = true;\r\n            // 记录状态\r\n            this.actionType = 'add';\r\n            console.log(typeof(this.userFormData.baseSalary))\r\n        },\r\n        //修改弹窗\r\n        edit(row) {\r\n            // 打开弹窗\r\n            this.dialogFormVisible = true\r\n            // 数据回显\r\n            this.userFormData.baseSalary = row.baseSalary\r\n            this.userFormData.allowance = row.allowance\r\n            this.userFormData.bonus = row.bonus\r\n            this.userFormData.subsidy = row.subsidy\r\n            this.userFormData.deduction = row.deduction\r\n            this.userFormData.personalIncomeTax = row.personalIncomeTax\r\n            this.userFormData.actualSalary = row.actualSalary\r\n            // 记录id\r\n            this.id = row.salaryID\r\n            console.log(row)\r\n            // 记录动作\r\n            this.actionType = 'edit'\r\n        },\r\n\r\n        calculatePersonalIncomeTax() {\r\n            // 先从 userFormData 中获取其他输入项的值\r\n            const { baseSalary, allowance, bonus, subsidy, deduction } = this.userFormData;\r\n\r\n            // 计算总收入\r\n            const totalIncome = baseSalary + allowance + bonus + subsidy - deduction;\r\n\r\n            // 计算税前应纳税额\r\n            const taxableIncome = totalIncome - 5000;\r\n\r\n            // 根据不同的情况计算 personalIncomeTax\r\n            if (taxableIncome > 0 && taxableIncome <= 5000) {\r\n                this.userFormData.personalIncomeTax = taxableIncome * 0;\r\n            } else if (taxableIncome > 5000 && taxableIncome <= 8000) {\r\n                this.userFormData.personalIncomeTax = taxableIncome * 0.03;\r\n            } else if (taxableIncome > 8000 && taxableIncome <= 17000) {\r\n                this.userFormData.personalIncomeTax = taxableIncome * 0.1;\r\n            } else if (taxableIncome > 17000) {\r\n                this.userFormData.personalIncomeTax = taxableIncome * 0.2;\r\n            }\r\n        },\r\n\r\n\r\n\r\n    },\r\n\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.row-box {\r\n    width: 100%;\r\n    display: flex;\r\n    margin-top: 20px;\r\n    margin-bottom: 8px;\r\n    justify-content: center;\r\n}\r\n</style>"],"mappings":"AA0GA,OAAAA,KAAA;AACA,OAAAC,KAAA;AACA;EAEAC,KAAA;IACA;MACAC,IAAA;MACA;MACAC,MAAA;MACAC,QAAA;MACAC,KAAA;MACA;MACAC,MAAA;QACAC,SAAA,EAAAP,KAAA,GAAAQ,QAAA,YAAAC,MAAA;QACAC,UAAA,EAAAV,KAAA,GAAAS,MAAA;MACA;MACAE,YAAA;QACAC,IAAA;QACAC,UAAA;QACAC,SAAA;QACAC,KAAA;QACAC,OAAA;QACAC,SAAA;QACAC,iBAAA;QACAC,YAAA;MACA;MACAC,MAAA,EAAAC,YAAA,CAAAC,OAAA;MACAC,UAAA,EAAAF,YAAA,CAAAC,OAAA;MACAE,UAAA;MACAC,EAAA;MACAC,iBAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;EACA;EAEAC,OAAA;IACA,MAAAD,QAAA;MAEA,MAAAE,eAAA;QACA,QAAAxB,MAAA;QACAC,SAAA,EAAAP,KAAA,MAAAM,MAAA,CAAAC,SAAA,EAAAE,MAAA;QACAC,UAAA,EAAAV,KAAA,MAAAM,MAAA,CAAAI,UAAA,EAAAD,MAAA;MACA;MACA,MAAAsB,GAAA,SAAAhC,KAAA;QACAiC,MAAA;QACAC,GAAA;QACA3B,MAAA;UACAH,MAAA,OAAAA,MAAA;UACAC,QAAA,OAAAA,QAAA;UACAgB,MAAA,OAAAA,MAAA;UACA,GAAAU;QACA;MACA;MACA;MACA,KAAA5B,IAAA,GAAA6B,GAAA,CAAA9B,IAAA,CAAAA,IAAA,CAAAC,IAAA,CAAAgC,GAAA,CAAAC,IAAA;QACA;UACA,GAAAA,IAAA;UACAC,UAAA,EAAApC,KAAA,CAAAmC,IAAA,CAAAC,UAAA,EAAA3B,MAAA;QACA;MACA;MACA4B,OAAA,CAAAC,GAAA,MAAApC,IAAA;MACA,KAAAG,KAAA,GAAA0B,GAAA,CAAA9B,IAAA,CAAAA,IAAA,CAAAI,KAAA;IACA;IACA;IACAkC,OAAA;MACA,KAAAX,OAAA;IACA;IACA;IACAY,WAAArC,MAAA;MACA;MACA,KAAAA,MAAA,GAAAA,MAAA;MACA;MACA,KAAAyB,OAAA;IACA;IACA;IACAa,QAAA;MACA;MACA,KAAAf,iBAAA;MACA;MACA,KAAAF,UAAA;MACAa,OAAA,CAAAC,GAAA,aAAA3B,YAAA,CAAAE,UAAA;IACA;IACA;IACA6B,KAAAC,GAAA;MACA;MACA,KAAAjB,iBAAA;MACA;MACA,KAAAf,YAAA,CAAAE,UAAA,GAAA8B,GAAA,CAAA9B,UAAA;MACA,KAAAF,YAAA,CAAAG,SAAA,GAAA6B,GAAA,CAAA7B,SAAA;MACA,KAAAH,YAAA,CAAAI,KAAA,GAAA4B,GAAA,CAAA5B,KAAA;MACA,KAAAJ,YAAA,CAAAK,OAAA,GAAA2B,GAAA,CAAA3B,OAAA;MACA,KAAAL,YAAA,CAAAM,SAAA,GAAA0B,GAAA,CAAA1B,SAAA;MACA,KAAAN,YAAA,CAAAO,iBAAA,GAAAyB,GAAA,CAAAzB,iBAA