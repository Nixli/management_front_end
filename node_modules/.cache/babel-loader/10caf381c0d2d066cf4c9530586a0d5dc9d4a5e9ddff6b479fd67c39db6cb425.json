{"ast":null,"code":"import axios from 'axios';\nimport '@fortawesome/fontawesome-free/css/all.css';\nimport { dataTool } from 'echarts';\nimport { Message } from 'element-ui';\nimport dayjs from 'dayjs';\nexport default {\n  data() {\n    return {\n      curState: '',\n      //isShow:false,\n      DeclarationDate: '',\n      ReviewDate: '',\n      // name: localStorage.getItem('name'),\n      count: \"报销金额\",\n      style: '报销类别',\n      reason: \"报销事由\",\n      note: \"备注\",\n      reimbursementID: '',\n      tableHeaderClassName: 'custom-table-header',\n      // 自定义的表头样式类名\n      list: [],\n      reimbursementID: '',\n      temp: {\n        amount: '',\n        category: '',\n        reason: '',\n        approvalStatus: '',\n        approver: '',\n        state: 0\n      },\n      //分页\n      pageno: 1,\n      pagesize: 5,\n      total: 0,\n      //搜索条件\n      params: {\n        approvalStatus: '',\n        assetName: dayjs().subtract(1, 'year').format('YYYY-MM-DD HH:mm:ss'),\n        brandModel: dayjs().format('YYYY-MM-DD HH:mm:ss')\n      },\n      show: false,\n      addshow: false\n    };\n  },\n  created() {\n    this.getList();\n  },\n  methods: {\n    async getList() {\n      const formattedParams = {\n        ...this.params,\n        assetName: dayjs(this.params.assetName).format('YYYY-MM-DD HH:mm:ss'),\n        brandModel: dayjs(this.params.brandModel).format('YYYY-MM-DD HH:mm:ss')\n      };\n      const res = await axios({\n        url: \"http://172.16.110.32:8080/Reimbursement/findAll\",\n        method: \"get\",\n        params: {\n          pageno: this.pageno,\n          pagesize: this.pagesize,\n          approvalStatus: this.approvalStatus,\n          ...formattedParams\n        }\n      });\n      this.list = res.data.data.map(item => {\n        return {\n          ...item,\n          createTime: dayjs(item.createTime).format('YYYY-MM-DD ')\n        };\n      });\n      this.total = res.data.data.total;\n    },\n    // 点击搜索\n    search() {\n      this.getList();\n    },\n    // 页码改变\n    pagechange(pageno) {\n      // 条件改变\n      this.pageno = pageno;\n      // 重新请求数据\n      this.getList();\n    },\n    updateRemibursement(row) {\n      console.log(row);\n      this.show = true;\n      this.userFormData.amount = row.amount;\n      this.userFormData.category = row.category;\n      this.userFormData.reason = row.reason;\n      this.userFormData.remarks = row.remarks;\n      this.reimbursementID = row.reimbursementID;\n    },\n    async deleteRemibursement(id) {\n      const data = {\n        salaryID: id\n      };\n      const res = await axios({\n        url: '/deleteSalary',\n        method: 'post',\n        data: data\n      });\n      this.getList();\n    },\n    async submit() {\n      const data = {\n        reimbursementID: this.reimbursementID,\n        ...this.userFormData\n      };\n      const res = await axios({\n        url: \"/updateReimbursement\",\n        method: \"post\",\n        data: data\n      });\n      this.getList();\n      this.show = false;\n    },\n    cancel() {\n      this.show = false;\n      this.addshow = false;\n    },\n    addopen() {\n      // 打开弹窗\n      this.show = true;\n      // 记录状态\n      this.actionType = 'add';\n      this.temp.amount = '';\n      this.temp.category = '';\n      this.temp.reason = '';\n      this.temp.approvalStatus = '';\n      this.temp.approver = '';\n      this.temp.remarks = '';\n    }\n  }\n};","map":{"version":3,"names":["axios","dataTool","Message","dayjs","data","curState","DeclarationDate","ReviewDate","count","style","reason","note","reimbursementID","tableHeaderClassName","list","temp","amount","category","approvalStatus","approver","state","pageno","pagesize","total","params","assetName","subtract","format","brandModel","show","addshow","created","getList","methods","formattedParams","res","url","method","map","item","createTime","search","pagechange","updateRemibursement","row","console","log","userFormData","remarks","deleteRemibursement","id","salaryID","submit","cancel","addopen","actionType"],"sources":["src/views/reimbursement/PersonalReimbursement.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <el-card>\r\n            <el-card>\r\n                <span style=\"font-size: 18px;font-weight: bold;font-family: 'Times New Roman', Times, serif;\">用户报销</span>\r\n                <el-popover placement=\"top-start\" title=\"报销功能介绍\" width=\"600\" trigger=\"hover\">\r\n                    <el-card>\r\n                        <span>摘要通过简要的文字描述,迅速明确交易内容，记录与该笔交易相关的业务事项</span><br>\r\n                        <span>在后续的财务核对和审计中，摘要是查找和理解交易内容的重要依据之一。</span><br>\r\n                        <span>清晰的摘要有助于提高财务数据的可读性和准确性。</span><br>\r\n                    </el-card>\r\n                    <i class=\"el-icon-info\" style=\"font-size: 16px; margin-left: 10px; margin-top: 5px;\"\r\n                        slot=\"reference\"></i>\r\n                </el-popover>\r\n\r\n            </el-card>\r\n            <el-card>\r\n                <div>\r\n                    <el-row type=\"flex\" justify=\"space-between\" align=\"middle\">\r\n                        <!-- 左侧部分，包括新增按钮 -->\r\n                         <el-col :span=\"23\">\r\n                        <el-form :inline=\"true\" style=\"margin-top: 20px;\">\r\n                            <el-form-item label=\"状态\">\r\n                                <el-input v-model=\"params.approvalStatus\" placeholder=\"报销状态\" style=\"width: 200px;\"/>\r\n                                <!-- <el-date-picker v-model=\"params.approvalStatus\" type=\"text\" placeholder=\"报销状态\">\r\n                                </el-date-picker> -->\r\n                            </el-form-item>\r\n                            <el-form-item label=\"日期\">\r\n                                <el-date-picker v-model=\"params.assetName\" type=\"date\" placeholder=\"开始日期\">\r\n                                </el-date-picker>\r\n                            </el-form-item>\r\n                            <el-form-item>\r\n                                <el-date-picker v-model=\"params.brandModel\" type=\"date\" placeholder=\"截止日期\">\r\n                                </el-date-picker>\r\n                            </el-form-item>\r\n                            <el-form-item>\r\n                                <el-button @click=\"search()\" type=\"primary\" icon=\"el-icon-search\">搜索</el-button>\r\n                            </el-form-item>\r\n\r\n                            <!-- 新增按钮 -->\r\n                            <el-button type=\"primary\" @click=\"addOpen\"><i\r\n                                    class=\"el-icon-circle-plus-outline el-icon--left\"></i>新增用户报销信息\r\n                            </el-button>\r\n                        </el-form>\r\n                    </el-col>\r\n                    </el-row>\r\n                </div>\r\n                <!-- 表格 -->\r\n                <el-table :data=\"list\" style=\"width: 100%;margin-top: 15px;\" :header-row-class-name=\"tableHeaderClassName\">\r\n                    <el-table-column prop=\"amount\" label=\"报销金额\" align=\"center\"> </el-table-column>\r\n                    <el-table-column prop=\"category\" label=\"报销类别\" align=\"center\"></el-table-column>\r\n                    <el-table-column prop=\"reason\" label=\"报销事由\" align=\"center\"></el-table-column>\r\n                    <el-table-column prop=\"approvalStatus\" label=\"审核状态\" align=\"center\"></el-table-column>\r\n                    <el-table-column prop=\"approver\" label=\"审核人\" align=\"center\"></el-table-column>\r\n                    <el-table-column prop=\"remarks\" label=\"备注\" align=\"center\"></el-table-column>\r\n                    <el-table-column prop=\"createTime\" label=\"创建时间\" align=\"center\"></el-table-column>\r\n                    <el-table-column label=\"操作\" align=\"center\" width=\"200px\">\r\n                        <template #default=\"{ row }\">\r\n                            <!-- 修改按钮 -->\r\n                           <el-button type=\"text\" icon=\"el-icon-edit-solid\" size=\"medium\"\r\n                                @click=\"updateRemibursement(row.remibursementID)\">修改</el-button>\r\n                        </template>\r\n                        <!-- 删除按钮 -->\r\n                        <template #default=\"{ row }\">\r\n                            <el-button type=\"text\" icon=\"el-icon-delete-solid\" size=\"medium\"\r\n                                @click=\"deleteRemibursement(row.remibursementID)\">删除</el-button>\r\n                        </template>\r\n                    </el-table-column>\r\n                </el-table>\r\n\r\n                <!-- 分页组件 -->\r\n                <el-pagination @current-change=\"pagechange\" background layout=\"prev, pager, next\" :total=\"total\"\r\n                    :page-size=\"pagesize\" style=\"text-align: center; margin-top: 10px;\">\r\n                </el-pagination>\r\n            </el-card>\r\n\r\n            <!-- 修改弹窗区域 -->\r\n            <el-dialog :title=\"actionType == 'add' ? '新增用户报销信息' : '编辑用户报销信息'\" :visible.sync=\"show\" width=\"900px\">\r\n                <!-- 表格 -->\r\n                 <el-form ref=\"dataForm\" :model=\"temp\" \r\n                    label-position=\"left\" label-width=\"70px\" style=\"width: 400px; margin-left:50px;\">\r\n                        <el-form-item label=\"报销金额\" prop=\"amount\">\r\n                            <el-input v-model=\"temp.amount\" />\r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"报销类别\" prop=\"category\">\r\n                            <el-input v-model=\"temp.category\" />\r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"报销事由\" prop=\"reason\">\r\n                             <el-input v-model=\"temp.reason\" />\r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"审核状态\" prop=\"approvalStatus\">\r\n                             <el-input v-model=\"temp.approvalStatus\" />\r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"审核人\" prop=\"approver\">\r\n                             <el-input v-model=\"temp.approver\" />\r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"备注\" prop=\"remarks\">\r\n                             <el-input v-model=\"temp.remarks\" />\r\n                        </el-form-item>\r\n                        \r\n                        <el-form-item>\r\n                            <el-button @click=\"submit\" style=\"width: 180px; margin-left: 110px\" type=\"primary\">提交<i\r\n                                class=\"el-icon-success el-icon--right\"></i></el-button>\r\n                        </el-form-item>\r\n                </el-form>\r\n            </el-dialog>\r\n        </el-card>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport '@fortawesome/fontawesome-free/css/all.css';\r\nimport { dataTool } from 'echarts';\r\nimport { Message } from 'element-ui'\r\nimport dayjs from 'dayjs';\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            curState:'',\r\n            //isShow:false,\r\n            DeclarationDate:'',\r\n            ReviewDate:'',\r\n            // name: localStorage.getItem('name'),\r\n            count: \"报销金额\",\r\n            style: '报销类别',\r\n            reason: \"报销事由\",\r\n            note: \"备注\",\r\n            reimbursementID: '',\r\n            tableHeaderClassName: 'custom-table-header',// 自定义的表头样式类名\r\n            list: [],\r\n            reimbursementID: '',\r\n            temp: {\r\n                amount: '',\r\n                category: '',\r\n                reason: '',\r\n                approvalStatus:'',\r\n                approver:'',\r\n                state: 0,\r\n            },\r\n            //分页\r\n            pageno: 1,\r\n            pagesize: 5,\r\n            total: 0,\r\n            //搜索条件\r\n            params: {\r\n                approvalStatus:'',\r\n                assetName: dayjs().subtract(1, 'year').format('YYYY-MM-DD HH:mm:ss'),\r\n                brandModel: dayjs().format('YYYY-MM-DD HH:mm:ss')\r\n            },\r\n            show: false,\r\n            addshow: false\r\n        }\r\n    },\r\n    created() {\r\n        this.getList()\r\n    },\r\n    methods: {\r\n\r\n        async getList() {\r\n            const formattedParams = {\r\n                ...this.params,\r\n                assetName: dayjs(this.params.assetName).format('YYYY-MM-DD HH:mm:ss'),\r\n                brandModel: dayjs(this.params.brandModel).format('YYYY-MM-DD HH:mm:ss'),\r\n            };\r\n            const res = await axios({\r\n                url: \"http://172.16.110.32:8080/Reimbursement/findAll\",\r\n                method: \"get\",\r\n                params: {\r\n                    pageno: this.pageno,\r\n                    pagesize: this.pagesize,\r\n                    approvalStatus: this.approvalStatus,\r\n                    ...formattedParams\r\n                }\r\n            });\r\n            this.list = res.data.data.map(item => {\r\n                return {\r\n                    ...item,\r\n                    createTime: dayjs(item.createTime).format('YYYY-MM-DD ')\r\n                };\r\n            });\r\n            this.total = res.data.data.total;\r\n        },\r\n        // 点击搜索\r\n        search() {\r\n            this.getList()\r\n        },\r\n        // 页码改变\r\n        pagechange(pageno) {\r\n            // 条件改变\r\n            this.pageno = pageno\r\n            // 重新请求数据\r\n            this.getList()\r\n        },\r\n\r\n        updateRemibursement(row) {\r\n            console.log(row)\r\n            this.show = true;\r\n            this.userFormData.amount = row.amount;\r\n            this.userFormData.category = row.category;\r\n            this.userFormData.reason = row.reason;\r\n            this.userFormData.remarks = row.remarks;\r\n            this.reimbursementID = row.reimbursementID\r\n        },\r\n        async deleteRemibursement(id) {\r\n            const data = {\r\n                salaryID: id\r\n            }\r\n            const res = await axios({\r\n                url: '/deleteSalary',\r\n                method: 'post',\r\n                data: data\r\n            })\r\n            this.getList()\r\n        },\r\n        async submit() {\r\n            const data = {\r\n                reimbursementID: this.reimbursementID,\r\n                ...this.userFormData\r\n            }\r\n            const res = await axios({\r\n                url: \"/updateReimbursement\",\r\n                method: \"post\",\r\n                data: data\r\n            })\r\n            this.getList()\r\n            this.show = false\r\n        },\r\n        cancel() {\r\n            this.show = false\r\n            this.addshow = false\r\n        },\r\n        addopen() {\r\n            // 打开弹窗\r\n            this.show = true;\r\n            // 记录状态\r\n            this.actionType = 'add';\r\n            this.temp.amount = '';\r\n            this.temp.category = '';\r\n            this.temp.reason = '';\r\n            this.temp.approvalStatus = '';\r\n            this.temp.approver = '';\r\n            this.temp.remarks = '';\r\n        }\r\n    }\r\n\r\n\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n::v-deep .el-input.is-disabled .el-input__inner {\r\n    background-color: #fff;\r\n    text-align: center;\r\n}\r\n\r\n.flex-container {\r\n    display: flex;\r\n}\r\n\r\n.charge-header {\r\n    margin-bottom: 10px;\r\n}\r\n\r\n.charge-header>div {\r\n    display: inline-block;\r\n    margin-right: 15px;\r\n}\r\n\r\n/* 修改标题行的颜色 */\r\n::v-deep .el-dialog__header {\r\n    background-color: rgb(243, 247, 249);\r\n    /* 替换为你想要的背景颜色值 */\r\n    color: #000000;\r\n    /* 替换为你想要的文字颜色值 */\r\n}\r\n\r\n::v-deep .custom-table-header th {\r\n    background-color: rgb(249, 251, 252);\r\n    /* 替换为你想要的背景颜色值 */\r\n    color: #000000;\r\n    /* 替换为你想要的文字颜色值 */\r\n}\r\n\r\n.button-group {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    /* 将按钮平均分布在弹窗底部 */\r\n    margin-top: 20px;\r\n    /* 为按钮组设置上边距，以调整按钮与输入框的间隔 */\r\n    margin-right: 80px;\r\n}\r\n</style>"],"mappings":"AAoHA,OAAAA,KAAA;AACA;AACA,SAAAC,QAAA;AACA,SAAAC,OAAA;AACA,OAAAC,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,QAAA;MACA;MACAC,eAAA;MACAC,UAAA;MACA;MACAC,KAAA;MACAC,KAAA;MACAC,MAAA;MACAC,IAAA;MACAC,eAAA;MACAC,oBAAA;MAAA;MACAC,IAAA;MACAF,eAAA;MACAG,IAAA;QACAC,MAAA;QACAC,QAAA;QACAP,MAAA;QACAQ,cAAA;QACAC,QAAA;QACAC,KAAA;MACA;MACA;MACAC,MAAA;MACAC,QAAA;MACAC,KAAA;MACA;MACAC,MAAA;QACAN,cAAA;QACAO,SAAA,EAAAtB,KAAA,GAAAuB,QAAA,YAAAC,MAAA;QACAC,UAAA,EAAAzB,KAAA,GAAAwB,MAAA;MACA;MACAE,IAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;EACA;EACAC,OAAA;IAEA,MAAAD,QAAA;MACA,MAAAE,eAAA;QACA,QAAAV,MAAA;QACAC,SAAA,EAAAtB,KAAA,MAAAqB,MAAA,CAAAC,SAAA,EAAAE,MAAA;QACAC,UAAA,EAAAzB,KAAA,MAAAqB,MAAA,CAAAI,UAAA,EAAAD,MAAA;MACA;MACA,MAAAQ,GAAA,SAAAnC,KAAA;QACAoC,GAAA;QACAC,MAAA;QACAb,MAAA;UACAH,MAAA,OAAAA,MAAA;UACAC,QAAA,OAAAA,QAAA;UACAJ,cAAA,OAAAA,cAAA;UACA,GAAAgB;QACA;MACA;MACA,KAAApB,IAAA,GAAAqB,GAAA,CAAA/B,IAAA,CAAAA,IAAA,CAAAkC,GAAA,CAAAC,IAAA;QACA;UACA,GAAAA,IAAA;UACAC,UAAA,EAAArC,KAAA,CAAAoC,IAAA,CAAAC,UAAA,EAAAb,MAAA;QACA;MACA;MACA,KAAAJ,KAAA,GAAAY,GAAA,CAAA/B,IAAA,CAAAA,IAAA,CAAAmB,KAAA;IACA;IACA;IACAkB,OAAA;MACA,KAAAT,OAAA;IACA;IACA;IACAU,WAAArB,MAAA;MACA;MACA,KAAAA,MAAA,GAAAA,MAAA;MACA;MACA,KAAAW,OAAA;IACA;IAEAW,oBAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;MACA,KAAAf,IAAA;MACA,KAAAkB,YAAA,CAAA/B,MAAA,GAAA4B,GAAA,CAAA5B,MAAA;MACA,KAAA+B,YAAA,CAAA9B,QAAA,GAAA2B,GAAA,CAAA3B,QAAA;MACA,KAAA8B,YAAA,CAAArC,MAAA,GAAAkC,GAAA,CAAAlC,MAAA;MACA,KAAAqC,YAAA,CAAAC,OAAA,GAAAJ,GAAA,CAAAI,OAAA;MACA,KAAApC,eAAA,GAAAgC,GAAA,CAAAhC,eAAA;IACA;IACA,MAAAqC,oBAAAC,EAAA;MACA,MAAA9C,IAAA;QACA+C,QAAA,EAAAD;MACA;MACA,MAAAf,GAAA,SAAAnC,KAAA;QACAoC,GAAA;QACAC,MAAA;QACAjC,IAAA,EAAAA;MACA;MACA,KAAA4B,OAAA;IACA;IACA,MA