{"ast":null,"code":"import axios from \"axios\";\nimport dayjs from \"dayjs\";\nexport default {\n  data() {\n    return {\n      DeclarationDate: '',\n      ReviewDate: '',\n      role: localStorage.getItem('role'),\n      list: [],\n      // 分页相关\n      pageno: 1,\n      pagesize: 5,\n      total: 0,\n      // 搜索条件\n      params: {\n        assetName: '',\n        brandModel: ''\n      },\n      // 控制弹框的显示与隐藏\n      dialogFormVisible: false,\n      // 新增表单的数据\n      userFormData: {\n        assetNumber: '',\n        assetName: '',\n        brandModel: '',\n        description: '',\n        originalValue: '',\n        netValue: '',\n        depreciationAmount: '',\n        accountBook: {\n          bookID: localStorage.getItem('bookID')\n        },\n        // 使用关系属性名\n        employee: {\n          employeeID: localStorage.getItem('employeeID')\n        }\n      },\n      // add： 新增   edit: '更新'\n      actionType: '',\n      id: '',\n      bookID: localStorage.getItem('bookID'),\n      employeeID: localStorage.getItem('employeeID'),\n      isShow: false,\n      name: '',\n      curState: ''\n    };\n  },\n  created() {\n    this.getList();\n    this.name = localStorage.getItem('name');\n  },\n  methods: {\n    // watch: {\n    //     userFormData: {\n    //         handler() {\n    //             /* console.log('数据发生变化了！！！') */\n    //             this.calculateDepreciationAmount()\n    //         },\n    //         deep: true\n    //     }\n    // },\n\n    // calculateDepreciationAmount() {\n    //     const { originalValue, netValue, depreciationAmount } = parseFloat(this.userFormData);\n    //     depreciationAmount = originalValue / netValue;\n\n    //     this.userFormData.depreciationAmount = depreciationAmount;\n    // },\n    async getList() {\n      const res = await axios({\n        method: \"get\",\n        url: \"/depreciationDetail\",\n        params: {\n          pageno: this.pageno,\n          pagesize: this.pagesize,\n          bookID: this.bookID,\n          employeeID: this.employeeID,\n          ...this.params\n        }\n      });\n\n      // 修改日期格式\n      this.list = res.data.data.list.map(item => {\n        return {\n          ...item,\n          ctime: dayjs(item.ctime).format('YYYY-MM-DD'),\n          utime: dayjs(item.utime).format('YYYY-MM-DD'),\n          netValue: dayjs(item.depreciationDetailContent.netValue).format('YYYY-MM-DD')\n        };\n      });\n      this.total = res.data.data.total;\n    },\n    // 点击搜索\n    search() {\n      this.getList();\n    },\n    // 页码改变\n    pagechange(pageno) {\n      // 条件改变\n      this.pageno = pageno;\n      // 重新请求数据\n      this.getList();\n    },\n    // 打开新增弹窗，\n    addOpen() {\n      // 打开弹窗\n      this.dialogFormVisible = true;\n      // 记录状态\n      this.actionType = 'add';\n      // 重置userFormData\n      this.userFormData = {\n        assetNumber: '',\n        assetName: '',\n        brandModel: '',\n        description: '',\n        originalValue: '',\n        netValue: '',\n        depreciationAmount: '',\n        accountBook: {\n          bookID: localStorage.getItem('bookID')\n        },\n        // 使用关系属性名\n        employeeID: this.employeeID\n      };\n    },\n    //新增或修改固定资产\n    async submit() {\n      // 拷贝userFormData到data\n      const data = {\n        ...this.userFormData\n      };\n      // 修改的时候需要把id带过去\n      const res = await axios({\n        url: '/depreciationDetail',\n        method: 'post',\n        data: data\n      });\n      this.dialogFormVisible = false;\n      this.getList();\n    },\n    process(row) {\n      console.log(row);\n      this.isShow = true;\n      this.DeclarationDate = row.ctime;\n      this.ReviewDate = row.utime;\n      this.curState = row.isRetired === 0 ? '待审核' : row.isRetired === 1 ? '已通过' : '未通过';\n    }\n  }\n};","map":{"version":3,"names":["axios","dayjs","data","DeclarationDate","ReviewDate","role","localStorage","getItem","list","pageno","pagesize","total","params","assetName","brandModel","dialogFormVisible","userFormData","assetNumber","description","originalValue","netValue","depreciationAmount","accountBook","bookID","employee","employeeID","actionType","id","isShow","name","curState","created","getList","methods","res","method","url","map","item","ctime","format","utime","depreciationDetailContent","search","pagechange","addOpen","submit","process","row","console","log","isRetired"],"sources":["src/views/fixedasset/PersonalDepreciation.vue"],"sourcesContent":["<template>\r\n    <div style=\"height: auto; width: auto;\">\r\n\r\n        <!-- 搜索新增区域 -->\r\n        <el-card>\r\n            <div style=\"display: flex; flex-direction: row;\">\r\n                <!-- 搜索区域 -->\r\n                <div style=\"flex: 8;\">\r\n                    <el-form :inline=\"true\">\r\n                        <el-form-item label=\"资产名称\">\r\n                            <el-input v-model=\"params.assetName\" placeholder=\"请输入资产名称\"></el-input>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"资产类型\">\r\n                            <el-input v-model=\"params.brandModel\" placeholder=\"请输入资产类型\"></el-input>\r\n                        </el-form-item>\r\n                        <el-form-item>\r\n                            <el-button @click=\"search\" type=\"primary\" icon=\"el-icon-search\">搜索</el-button>\r\n                        </el-form-item>\r\n                        <el-button type=\"primary\" @click=\"addOpen\">新增折旧申请</el-button>\r\n                    </el-form>\r\n                </div>\r\n\r\n            </div>\r\n        </el-card>\r\n\r\n        <!-- 折旧明细表 -->\r\n        <el-card style=\"margin-top: 20px;\">\r\n\r\n            <el-card>\r\n                <span style=\"font-size: 16px;font-weight: bold;font-family: 'Times New Roman', Times, serif;\">折旧明细表</span>\r\n            </el-card>\r\n\r\n            <el-table :data=\"list\" border style=\"width: 100%\">\r\n                <el-table-column type=\"expand\">\r\n                    <template #default=\"{ row }\">\r\n                        <!-- <el-form label-position=\"left\" inline class=\"demo-table-expand\">\r\n                            <el-form-item label=\"商品名称 所属店铺 商品 ID 商品分类 店铺地址\">\r\n                                <span>{{ row.depreciationDetailContent }}</span>\r\n                            </el-form-item>\r\n                        </el-form> -->\r\n                        <div style=\"padding: 10px;\">\r\n                            <el-table :data=\"row.depreciationDetailContent\">\r\n                                <el-table-column label=\"日期\" prop=\"netValue\"></el-table-column>\r\n                                <el-table-column label=\"每月折旧\" prop=\"depreciationAmount\"></el-table-column>\r\n                            </el-table>\r\n                        </div>\r\n                    </template>\r\n                </el-table-column>\r\n\r\n                <el-table-column prop=\"assetName\" label=\"资产名称\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"brandModel\" label=\"资产类型\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"description\" label=\"品牌型号\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"originalValue\" label=\"资产原值\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"netValue\" label=\"折旧月份\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"depreciationAmount\" label=\"每月折旧\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"ctime\" label=\"申请日期\" align=\"center\"> </el-table-column>\r\n                <el-table-column prop=\"isRetired\" label=\"状态\" align=\"center\">\r\n                    <template #default=\"{ row }\">\r\n                        {{ row.isRetired === 0 ? '待审核' : (row.isRetired === 1 ? '已通过' : '未通过') }}\r\n                    </template>\r\n                </el-table-column>\r\n                <el-table-column fixed=\"right\" label=\"流程\">\r\n                    <template #default=\"{ row }\">\r\n                        <!-- 在这里使用 row 对象，表示整行的数据 -->\r\n                        <el-button type=\"text\" size=\"small\" @click=\"process(row)\">审批流程</el-button>\r\n                    </template>\r\n                </el-table-column>\r\n\r\n            </el-table>\r\n\r\n            <!-- 分页组件 -->\r\n            <el-pagination @current-change=\"pagechange\" background layout=\"prev, pager, next\" :total=\"total\"\r\n                :page-size=\"pagesize\" style=\"text-align: center; margin-top: 10px;\">\r\n            </el-pagination>\r\n        </el-card>\r\n\r\n        <!-- 弹窗区域 -->\r\n        <el-dialog :title=\"actionType == 'add' ? '新增折旧明细' : '编辑折旧明细'\" :visible.sync=\"dialogFormVisible\">\r\n            <el-form :model=\"userFormData\" class=\"demo-form-inline\" label-width=\"120px\">\r\n\r\n                <el-form-item label=\"资产名称\">\r\n                    <el-input v-model=\"userFormData.assetName\" placeholder=\"资产名称\"></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"资产类型\">\r\n                    <el-input v-model=\"userFormData.brandModel\" placeholder=\"资产类型\"></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"品牌型号\">\r\n                    <el-input v-model=\"userFormData.description\" placeholder=\"品牌型号\"></el-input>\r\n                </el-form-item>\r\n\r\n                <el-form-item label=\"资产原值\">\r\n                    <el-input v-model=\"userFormData.originalValue\" placeholder=\"资产原值\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"折旧月份\">\r\n                    <el-input-number v-model=\"userFormData.netValue\" placeholder=\"折旧月份\"></el-input-number>\r\n                </el-form-item>\r\n                <el-form-item label=\"每月折旧\">\r\n                    <el-input-number v-model=\"userFormData.depreciationAmount\" placeholder=\"每月折旧\"></el-input-number>\r\n                </el-form-item>\r\n\r\n                <el-form-item>\r\n                    <el-button @click=\"submit\">提交</el-button>\r\n                </el-form-item>\r\n\r\n            </el-form>\r\n        </el-dialog>\r\n\r\n        <el-dialog title=\"折旧流程\" :visible.sync=\"isShow\">\r\n            <div class=\"block\">\r\n                <el-timeline>\r\n                    <el-timeline-item :timestamp=\"DeclarationDate\" placement=\"top\">\r\n                        <el-card>\r\n                            <p>员工 {{ name }} 提交申请于 {{ DeclarationDate }}</p>\r\n                        </el-card>\r\n                    </el-timeline-item>\r\n                    <el-timeline-item v-if=\"curState != '待审核'\" :timestamp=\"ReviewDate\" placement=\"top\">\r\n                        <el-card>\r\n                            <p>老板 审核申请于 {{ ReviewDate }}</p>\r\n                        </el-card>\r\n                    </el-timeline-item>\r\n                </el-timeline>\r\n            </div>\r\n        </el-dialog>\r\n\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\nimport dayjs from \"dayjs\";\r\n\r\nexport default {\r\n\r\n    data() {\r\n        return {\r\n            DeclarationDate: '',\r\n            ReviewDate: '',\r\n            role: localStorage.getItem('role'),\r\n            list: [],\r\n            // 分页相关\r\n            pageno: 1,\r\n            pagesize: 5,\r\n            total: 0,\r\n            // 搜索条件\r\n            params: {\r\n                assetName: '',\r\n                brandModel: ''\r\n            },\r\n            // 控制弹框的显示与隐藏\r\n            dialogFormVisible: false,\r\n            // 新增表单的数据\r\n            userFormData: {\r\n                assetNumber: '',\r\n                assetName: '',\r\n                brandModel: '',\r\n                description: '',\r\n                originalValue: '',\r\n                netValue: '',\r\n                depreciationAmount: '',\r\n                accountBook: { bookID: localStorage.getItem('bookID') }, // 使用关系属性名\r\n                employee: { employeeID: localStorage.getItem('employeeID') }\r\n            },\r\n            // add： 新增   edit: '更新'\r\n            actionType: '',\r\n            id: '',\r\n            bookID: localStorage.getItem('bookID'),\r\n            employeeID: localStorage.getItem('employeeID'),\r\n            isShow: false,\r\n            name: '',\r\n            curState: ''\r\n        };\r\n    },\r\n    created() {\r\n        this.getList();\r\n        this.name = localStorage.getItem('name')\r\n    },\r\n\r\n    methods: {\r\n\r\n        // watch: {\r\n        //     userFormData: {\r\n        //         handler() {\r\n        //             /* console.log('数据发生变化了！！！') */\r\n        //             this.calculateDepreciationAmount()\r\n        //         },\r\n        //         deep: true\r\n        //     }\r\n        // },\r\n\r\n        // calculateDepreciationAmount() {\r\n        //     const { originalValue, netValue, depreciationAmount } = parseFloat(this.userFormData);\r\n        //     depreciationAmount = originalValue / netValue;\r\n\r\n        //     this.userFormData.depreciationAmount = depreciationAmount;\r\n        // },\r\n        async getList() {\r\n            const res = await axios({\r\n                method: \"get\",\r\n                url: \"/depreciationDetail\",\r\n                params: {\r\n                    pageno: this.pageno,\r\n                    pagesize: this.pagesize,\r\n                    bookID: this.bookID,\r\n                    employeeID: this.employeeID,\r\n                    ...this.params,\r\n                },\r\n            });\r\n\r\n            // 修改日期格式\r\n            this.list = res.data.data.list.map(item => {\r\n                return {\r\n                    ...item,\r\n                    ctime: dayjs(item.ctime).format('YYYY-MM-DD'),\r\n                    utime: dayjs(item.utime).format('YYYY-MM-DD'),\r\n                    netValue:dayjs(item.depreciationDetailContent.netValue).format('YYYY-MM-DD')\r\n                };\r\n            });\r\n            this.total = res.data.data.total;\r\n        },\r\n        // 点击搜索\r\n        search() {\r\n            this.getList()\r\n        },\r\n        // 页码改变\r\n        pagechange(pageno) {\r\n            // 条件改变\r\n            this.pageno = pageno\r\n            // 重新请求数据\r\n            this.getList()\r\n        },\r\n        // 打开新增弹窗，\r\n        addOpen() {\r\n            // 打开弹窗\r\n            this.dialogFormVisible = true\r\n            // 记录状态\r\n            this.actionType = 'add'\r\n            // 重置userFormData\r\n            this.userFormData = {\r\n                assetNumber: '',\r\n                assetName: '',\r\n                brandModel: '',\r\n                description: '',\r\n                originalValue: '',\r\n                netValue: '',\r\n                depreciationAmount: '',\r\n                accountBook: { bookID: localStorage.getItem('bookID') }, // 使用关系属性名\r\n                employeeID: this.employeeID\r\n            }\r\n        },\r\n\r\n        //新增或修改固定资产\r\n        async submit() {\r\n            // 拷贝userFormData到data\r\n            const data = { ...this.userFormData }\r\n            // 修改的时候需要把id带过去\r\n            const res = await axios({\r\n                url: '/depreciationDetail',\r\n                method: 'post',\r\n                data: data\r\n            })\r\n            this.dialogFormVisible = false\r\n            this.getList()\r\n        },\r\n        process(row) {\r\n            console.log(row)\r\n            this.isShow = true\r\n            this.DeclarationDate = row.ctime\r\n            this.ReviewDate = row.utime\r\n            this.curState = row.isRetired === 0 ? '待审核' : (row.isRetired === 1 ? '已通过' : '未通过')\r\n        }\r\n    },\r\n\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.row-box {\r\n    width: 100%;\r\n    display: flex;\r\n    margin-top: 20px;\r\n    margin-bottom: 8px;\r\n    justify-content: center;\r\n}\r\n</style>"],"mappings":"AAoIA,OAAAA,KAAA;AACA,OAAAC,KAAA;AAEA;EAEAC,KAAA;IACA;MACAC,eAAA;MACAC,UAAA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA;MACAC,IAAA;MACA;MACAC,MAAA;MACAC,QAAA;MACAC,KAAA;MACA;MACAC,MAAA;QACAC,SAAA;QACAC,UAAA;MACA;MACA;MACAC,iBAAA;MACA;MACAC,YAAA;QACAC,WAAA;QACAJ,SAAA;QACAC,UAAA;QACAI,WAAA;QACAC,aAAA;QACAC,QAAA;QACAC,kBAAA;QACAC,WAAA;UAAAC,MAAA,EAAAjB,YAAA,CAAAC,OAAA;QAAA;QAAA;QACAiB,QAAA;UAAAC,UAAA,EAAAnB,YAAA,CAAAC,OAAA;QAAA;MACA;MACA;MACAmB,UAAA;MACAC,EAAA;MACAJ,MAAA,EAAAjB,YAAA,CAAAC,OAAA;MACAkB,UAAA,EAAAnB,YAAA,CAAAC,OAAA;MACAqB,MAAA;MACAC,IAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;IACA,KAAAH,IAAA,GAAAvB,YAAA,CAAAC,OAAA;EACA;EAEA0B,OAAA;IAEA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;;IAEA;IACA;IACA;;IAEA;IACA;IACA,MAAAD,QAAA;MACA,MAAAE,GAAA,SAAAlC,KAAA;QACAmC,MAAA;QACAC,GAAA;QACAxB,MAAA;UACAH,MAAA,OAAAA,MAAA;UACAC,QAAA,OAAAA,QAAA;UACAa,MAAA,OAAAA,MAAA;UACAE,UAAA,OAAAA,UAAA;UACA,QAAAb;QACA;MACA;;MAEA;MACA,KAAAJ,IAAA,GAAA0B,GAAA,CAAAhC,IAAA,CAAAA,IAAA,CAAAM,IAAA,CAAA6B,GAAA,CAAAC,IAAA;QACA;UACA,GAAAA,IAAA;UACAC,KAAA,EAAAtC,KAAA,CAAAqC,IAAA,CAAAC,KAAA,EAAAC,MAAA;UACAC,KAAA,EAAAxC,KAAA,CAAAqC,IAAA,CAAAG,KAAA,EAAAD,MAAA;UACApB,QAAA,EAAAnB,KAAA,CAAAqC,IAAA,CAAAI,yBAAA,CAAAtB,QAAA,EAAAoB,MAAA;QACA;MACA;MACA,KAAA7B,KAAA,GAAAuB,GAAA,CAAAhC,IAAA,CAAAA,IAAA,CAAAS,KAAA;IACA;IACA;IACAgC,OAAA;MACA,KAAAX,OAAA;IACA;IACA;IACAY,WAAAnC,MAAA;MACA;MACA,KAAAA,MAAA,GAAAA,MAAA;MACA;MACA,KAAAuB,OAAA;IACA;IACA;IACAa,QAAA;MACA;MACA,KAAA9B,iBAAA;MACA;MACA,KAAAW,UAAA;MACA;MACA,KAAAV,YAAA;QACAC,WAAA;QACAJ,SAAA;QACAC,UAAA;QACAI,WAAA;QACAC,aAAA;QACAC,QAAA;QACAC,kBAAA;QACAC,WAAA;UAAAC,MAAA,EAAAjB,YAAA,CAAAC,OAAA;QAAA;QAAA;QACAkB,UAAA,OAAAA;MACA;IACA;IAEA;IACA,MAAAqB,OAAA;MACA;MACA,MAAA5C,IAAA;QAAA,QAAAc;MAAA;MACA;MACA,MAAAkB,GAAA,SAAAlC,KAAA;QACAoC,GAAA;QACAD,MAAA;QACAjC,IAAA,EAAAA;MACA;MACA,KAAAa,iBAAA;MACA,KAAAiB,OAAA;IACA;IACAe,QAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;M