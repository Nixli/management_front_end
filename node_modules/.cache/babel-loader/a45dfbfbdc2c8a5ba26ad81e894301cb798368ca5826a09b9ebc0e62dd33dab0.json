{"ast":null,"code":"import axios from 'axios';\nimport dayjs from 'dayjs';\nexport default {\n  data() {\n    return {\n      statue0: false,\n      statue1: false,\n      statue2: false,\n      totalshouAmountSum: 0,\n      totalfuAmountSum: 0,\n      VAT: [],\n      bookID: localStorage.getItem('bookID'),\n      xxlist: []\n    };\n  },\n  methods: {\n    totalNumber() {\n      let totalScore = this.xxlist.reduce((pre, cur) => {\n        return pre + cur.depreciationAmount;\n      }, 0);\n    },\n    async getxxList() {\n      const res = await axios({\n        method: \"get\",\n        url: \"/xxdepreciationDetail\",\n        params: {\n          bookID: this.bookID\n        }\n      });\n\n      // 修改日期格式\n      this.xxlist = res.data.data.list.map(item => {\n        return {\n          depreciationAmount: item.depreciationAmount\n        };\n      });\n      this.total = res.data.data.total;\n    },\n    isDayu0(totalshouAmountSum, totalfuAmountSum) {\n      if (totalshouAmountSum - totalfuAmountSum < 0) {\n        return 0;\n      } else {\n        return (totalshouAmountSum - totalfuAmountSum) * 0.13;\n      }\n    },\n    update0() {\n      this.statue0 = true;\n    },\n    update1() {\n      this.statue1 = true;\n    },\n    update2() {\n      this.statue2 = true;\n    },\n    async getShouVoucherList() {\n      const currentYearMonth = dayjs().format('YYYY-MM');\n      const res = await axios({\n        url: \"/voucher\",\n        method: \"get\",\n        params: {\n          bookID: localStorage.getItem('bookID'),\n          voucherWord: '收'\n        }\n      });\n      this.VAT = res.data.data.list.map(item => ({\n        ...item,\n        cime: dayjs(item.cime).format('YYYY-MM')\n      }));\n      this.VAT = this.VAT.filter(item => {\n        // 比较年份月份是否与当前年份月份一致\n        return item.cime === currentYearMonth;\n      });\n      console.log(this.VAT);\n      this.totalshouAmountSum = this.VAT.reduce((accumulator, currentValue) => {\n        // 将字符串形式的金额转换为浮点数并累计\n        const amount = parseFloat(currentValue.totalAmount.replace(',', '')) || 0;\n        return accumulator + amount;\n      }, 0);\n    },\n    async getFuVoucherList() {\n      const currentYearMonth = dayjs().format('YYYY-MM');\n      const res = await axios({\n        url: \"/voucher\",\n        method: \"get\",\n        params: {\n          bookID: localStorage.getItem('bookID'),\n          voucherWord: '付'\n        }\n      });\n      this.VAT = res.data.data.list.map(item => ({\n        ...item,\n        cime: dayjs(item.cime).format('YYYY-MM')\n      }));\n      this.VAT = this.VAT.filter(item => {\n        // 比较年份月份是否与当前年份月份一致\n        return item.cime === currentYearMonth;\n      });\n      this.totalfuAmountSum = this.VAT.reduce((accumulator, currentValue) => {\n        // 将字符串形式的金额转换为浮点数并累计\n        const amount = parseFloat(currentValue.totalAmount.replace(',', '')) || 0;\n        return accumulator + amount;\n      }, 0);\n    }\n  },\n  created() {\n    this.getShouVoucherList();\n    this.getFuVoucherList();\n    this.getxxList();\n  }\n};","map":{"version":3,"names":["axios","dayjs","data","statue0","statue1","statue2","totalshouAmountSum","totalfuAmountSum","VAT","bookID","localStorage","getItem","xxlist","methods","totalNumber","totalScore","reduce","pre","cur","depreciationAmount","getxxList","res","method","url","params","list","map","item","total","isDayu0","update0","update1","update2","getShouVoucherList","currentYearMonth","format","voucherWord","cime","filter","console","log","accumulator","currentValue","amount","parseFloat","totalAmount","replace","getFuVoucherList","created"],"sources":["src/views/carryover/CarryOver.vue"],"sourcesContent":["<template>\r\n    <el-card class=\"test\">\r\n        <div slot=\"header\"><span>期末结转</span></div>\r\n        <el-row :gutter=\"20\">\r\n            <el-col :span=\"8\">\r\n                <el-card class=\"box-card\">\r\n                    <div slot=\"header\" class=\"clearfix\">\r\n                        <span>计提折旧</span>\r\n                        <el-button style=\"float: right; padding: 3px 0\" type=\"text\" @click=\"update0()\">测算金额</el-button>\r\n                    </div>\r\n                    <div class=\"text item\" v-if=\"statue0 === true\">\r\n                        {{ this.totalNumber() }}\r\n                        {{  this.xxlist  }}\r\n                    </div>\r\n                </el-card>\r\n            </el-col>\r\n            <el-col :span=\"8\">\r\n                <el-card class=\"box-card\">\r\n                    <div slot=\"header\" class=\"clearfix\">\r\n                        <span>计提增值税</span>\r\n                        <el-button style=\"float: right; padding: 3px 0\" type=\"text\" @click=\"update1()\">测算金额</el-button>\r\n                    </div>\r\n                    <div class=\"text item\" v-if=\"statue1 === true\">\r\n                        {{ parseFloat((totalshouAmountSum / 1.13 * 0.13).toFixed(2)) }}\r\n                    </div>\r\n                </el-card>\r\n            </el-col>\r\n            <el-col :span=\"8\">\r\n                <el-card class=\"box-card\">\r\n                    <div slot=\"header\" class=\"clearfix\">\r\n                        <span>计提所得税</span>\r\n                        <el-button style=\"float: right; padding: 3px 0\" type=\"text\" @click=\"update2()\">测算金额</el-button>\r\n                    </div>\r\n                    <div class=\"text item\" v-if=\"statue2 === true\">\r\n                        {{ isDayu0(this.totalshouAmountSum, this.totalfuAmountSum) }}\r\n                    </div>\r\n                </el-card>\r\n            </el-col>\r\n        </el-row>\r\n        \r\n    </el-card>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport dayjs from 'dayjs';\r\n\r\nexport default {\r\n\r\n    data() {\r\n        return {\r\n            statue0: false,\r\n            statue1: false,\r\n            statue2: false,\r\n            totalshouAmountSum: 0,\r\n            totalfuAmountSum: 0,\r\n            VAT: [],\r\n            bookID: localStorage.getItem('bookID'),\r\n            xxlist:[],\r\n            \r\n        }\r\n    },\r\n    methods: {\r\n\r\n        totalNumber(){\r\n            let totalScore=this.xxlist.reduce((pre,cur)=>{\r\n            return pre+cur.depreciationAmount\r\n        },0)\r\n        },\r\n        async getxxList() {\r\n            const res = await axios({\r\n                method: \"get\",\r\n                url: \"/xxdepreciationDetail\",\r\n                params: {\r\n                    bookID: this.bookID,\r\n                },\r\n            });\r\n\r\n            // 修改日期格式\r\n            this.xxlist = res.data.data.list.map(item => {\r\n                return {\r\n                    depreciationAmount:item.depreciationAmount\r\n                };\r\n            });\r\n            this.total = res.data.data.total;\r\n        },\r\n\r\n        isDayu0(totalshouAmountSum, totalfuAmountSum) {\r\n            if (totalshouAmountSum - totalfuAmountSum < 0) {\r\n                return 0;\r\n            } else {\r\n                return (totalshouAmountSum - totalfuAmountSum) * 0.13;\r\n            }\r\n        },\r\n        update0() {\r\n            this.statue0 = true;\r\n        },\r\n        update1() {\r\n            this.statue1 = true;\r\n        },\r\n        update2() {\r\n            this.statue2 = true;\r\n        },\r\n        async getShouVoucherList() {\r\n            const currentYearMonth = dayjs().format('YYYY-MM');\r\n            const res = await axios({\r\n                url: \"/voucher\",\r\n                method: \"get\",\r\n                params: {\r\n                    bookID: localStorage.getItem('bookID'),\r\n                    voucherWord: '收'\r\n                }\r\n            })\r\n            this.VAT = res.data.data.list.map(item => ({\r\n                ...item,\r\n                cime: dayjs(item.cime).format('YYYY-MM')\r\n            }))\r\n            this.VAT = this.VAT.filter(item => {\r\n                // 比较年份月份是否与当前年份月份一致\r\n                return item.cime === currentYearMonth;\r\n            });\r\n            console.log(this.VAT)\r\n\r\n            this.totalshouAmountSum = this.VAT.reduce((accumulator, currentValue) => {\r\n                // 将字符串形式的金额转换为浮点数并累计\r\n                const amount = parseFloat(currentValue.totalAmount.replace(',', '')) || 0;\r\n                return accumulator + amount;\r\n            }, 0);\r\n        },\r\n\r\n        async getFuVoucherList() {\r\n            const currentYearMonth = dayjs().format('YYYY-MM');\r\n            const res = await axios({\r\n                url: \"/voucher\",\r\n                method: \"get\",\r\n                params: {\r\n                    bookID: localStorage.getItem('bookID'),\r\n                    voucherWord: '付'\r\n                }\r\n            })\r\n            this.VAT = res.data.data.list.map(item => ({\r\n                ...item,\r\n                cime: dayjs(item.cime).format('YYYY-MM')\r\n            }))\r\n            this.VAT = this.VAT.filter(item => {\r\n                // 比较年份月份是否与当前年份月份一致\r\n                return item.cime === currentYearMonth;\r\n            });\r\n\r\n            this.totalfuAmountSum = this.VAT.reduce((accumulator, currentValue) => {\r\n                // 将字符串形式的金额转换为浮点数并累计\r\n                const amount = parseFloat(currentValue.totalAmount.replace(',', '')) || 0;\r\n                return accumulator + amount;\r\n            }, 0);\r\n        },\r\n\r\n    },\r\n    created() {\r\n        this.getShouVoucherList();\r\n        this.getFuVoucherList();\r\n        this.getxxList();\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n::v-deep .el-card__header {\r\n    background-color: rgb(249, 251, 252);\r\n}\r\n\r\n.text {\r\n    font-size: 24px;\r\n    text-align: center;\r\n    justify-content: center;\r\n    margin-top: 55px;\r\n\r\n}\r\n\r\n.item {\r\n    margin-bottom: 18px;\r\n}\r\n\r\n.clearfix:before,\r\n.clearfix:after {\r\n    display: table;\r\n    content: \"\";\r\n}\r\n\r\n.clearfix:after {\r\n    clear: both\r\n}\r\n\r\n.box-card {\r\n    width: 350px;\r\n    height: 270px;\r\n}\r\n\r\n.el-row {\r\n    margin-bottom: 20px;\r\n\r\n    &:last-child {\r\n        margin-bottom: 0;\r\n    }\r\n}\r\n\r\n.el-col {\r\n    border-radius: 4px;\r\n}\r\n\r\n.bg-purple-dark {\r\n    background: #99a9bf;\r\n}\r\n\r\n.bg-purple {\r\n    background: #d3dce6;\r\n}\r\n\r\n.bg-purple-light {\r\n    background: #e5e9f2;\r\n}\r\n\r\n.grid-content {\r\n    border-radius: 4px;\r\n    min-height: 36px;\r\n}\r\n\r\n.row-bg {\r\n    padding: 10px 0;\r\n    background-color: #f9fafc;\r\n}\r\n</style>"],"mappings":"AA4CA,OAAAA,KAAA;AACA,OAAAC,KAAA;AAEA;EAEAC,KAAA;IACA;MACAC,OAAA;MACAC,OAAA;MACAC,OAAA;MACAC,kBAAA;MACAC,gBAAA;MACAC,GAAA;MACAC,MAAA,EAAAC,YAAA,CAAAC,OAAA;MACAC,MAAA;IAEA;EACA;EACAC,OAAA;IAEAC,YAAA;MACA,IAAAC,UAAA,QAAAH,MAAA,CAAAI,MAAA,EAAAC,GAAA,EAAAC,GAAA;QACA,OAAAD,GAAA,GAAAC,GAAA,CAAAC,kBAAA;MACA;IACA;IACA,MAAAC,UAAA;MACA,MAAAC,GAAA,SAAArB,KAAA;QACAsB,MAAA;QACAC,GAAA;QACAC,MAAA;UACAf,MAAA,OAAAA;QACA;MACA;;MAEA;MACA,KAAAG,MAAA,GAAAS,GAAA,CAAAnB,IAAA,CAAAA,IAAA,CAAAuB,IAAA,CAAAC,GAAA,CAAAC,IAAA;QACA;UACAR,kBAAA,EAAAQ,IAAA,CAAAR;QACA;MACA;MACA,KAAAS,KAAA,GAAAP,GAAA,CAAAnB,IAAA,CAAAA,IAAA,CAAA0B,KAAA;IACA;IAEAC,QAAAvB,kBAAA,EAAAC,gBAAA;MACA,IAAAD,kBAAA,GAAAC,gBAAA;QACA;MACA;QACA,QAAAD,kBAAA,GAAAC,gBAAA;MACA;IACA;IACAuB,QAAA;MACA,KAAA3B,OAAA;IACA;IACA4B,QAAA;MACA,KAAA3B,OAAA;IACA;IACA4B,QAAA;MACA,KAAA3B,OAAA;IACA;IACA,MAAA4B,mBAAA;MACA,MAAAC,gBAAA,GAAAjC,KAAA,GAAAkC,MAAA;MACA,MAAAd,GAAA,SAAArB,KAAA;QACAuB,GAAA;QACAD,MAAA;QACAE,MAAA;UACAf,MAAA,EAAAC,YAAA,CAAAC,OAAA;UACAyB,WAAA;QACA;MACA;MACA,KAAA5B,GAAA,GAAAa,GAAA,CAAAnB,IAAA,CAAAA,IAAA,CAAAuB,IAAA,CAAAC,GAAA,CAAAC,IAAA;QACA,GAAAA,IAAA;QACAU,IAAA,EAAApC,KAAA,CAAA0B,IAAA,CAAAU,IAAA,EAAAF,MAAA;MACA;MACA,KAAA3B,GAAA,QAAAA,GAAA,CAAA8B,MAAA,CAAAX,IAAA;QACA;QACA,OAAAA,IAAA,CAAAU,IAAA,KAAAH,